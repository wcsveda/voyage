.material-icons {
    color:white;
    vertical-align: bottom; 
    padding-right: 0.2em;
}

#top-nav {
    background-color: #2D3E50;
    padding: 2%;
    overflow: hidden;
    font-family: monospace;

    & ul {
        @util reset-list;

        & li {
            display: inline-block;
            color:white;

            & a {
                color: inherit;
                text-decoration: none;
                text-align: center;
                display: inline-block;
            }
        }
    }
    
#register_link {
    color:#1DD2AF;
}
}


